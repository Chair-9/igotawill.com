<div class="container">
  <div class="jumbotron">
    <h1>My Will</h1>
      <dl class="dl-horizontal">
        <dt>Title</dt>
        <dd><%= @will.title %></dd>
        <dt>Date</dt>
        <dd><%= @will.date %></dd>
        <dt>Executor</dt>
        <dd><%= @will.executor %></dd>
        <dt>Estate Plan</dt>
        <dd><%= @will.estate_plan %></dd>
        <dt>Comments</dt>
        <dd><%= @will.comments %></dd>
      </dl>
      <br />
      <%= link_to "Edit Will", edit_will_path %>
        <p><a class="btn btn-primary btn-lg">Learn more</a></p>
  </div>
</div>
