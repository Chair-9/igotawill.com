<div class="container">
  <div class="col-md-8 col-sm-offset-2">
    <div class="well">
      <h1>Update User Info</h1>
      <%= simple_form_for @user do |f| %>
        <%= f.input :first_name %>
        <%= f.input :last_name %>
        <%= f.input :estate_plan, as: :text %>
        <%= f.input :comments, as: :text %>

        <%= f.button :submit, class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>
